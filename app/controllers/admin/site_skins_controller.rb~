class Admin::SiteSkinsController < ApplicationController
  only_allow_access_to :index, :show, :new, :create, :edit, :update, :destroy,
    :when => :admin,
    :denied_message => 'You must have administrative privileges to edit Skins.'
  skip_before_filter :verify_authenticity_token, :only => [:users, :remove_user, :add_user]

	def activate
		@skin = Skin.find(params[:id])
		debugger
	end
end
